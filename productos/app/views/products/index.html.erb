<nav class="navbar navbar-inverse navbar-static-top prods">
      <div class="container">
        <div class="navbar-collapse collapse" id="navbar">
          <ul class="nav navbar-nav">
            <li class="active"><h1>Listing Products</h1></li>
          </ul>
        </div><!--/.nav-collapse -->
      </div>
    </nav>

<div class="container prods">
<p id="notice"><%= notice %></p>
  <table class="table">
    <thead>
      <tr>
        <th>Name</th>
        <th>Description</th>
        <th>Price</th>
        <th>Published</th>
        <th colspan="3"></th>
      </tr>
    </thead>

    <tbody>
      <% @products.each do |product| %>
        <tr>
          <td><%= product.name %></td>
          <td><%= product.description %></td>
          <td><%= product.price %></td>
          <td><%= product.published %></td>
          <td><%= link_to 'Show', product, :class => 'btn btn-primary' %></td>
          <td><%= link_to 'Edit', edit_product_path(product), :class => 'btn btn-info' %></td>
          <td><%= link_to 'Destroy', product, method: :delete, data: { confirm: 'Are you sure?' }, :class => 'btn btn-danger' %></td>
        </tr>
      <% end %>
    </tbody>
  </table>

  <br>

  <%= link_to 'New Product', new_product_path, :class => 'btn btn-success' %>
  
</div>




